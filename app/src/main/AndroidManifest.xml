<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buster.mycrews">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label=""
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.User.MyCrewsActivity" />
        <activity android:name=".UI.Crew.FindCrewActivity" />
        <activity android:name=".UI.User.MyProfileActivity" />
        <activity android:name=".UI.Crew.CrewProfileActivity" />
        <activity android:name=".UI.User.MyEditProfileActivity" />
        <activity android:name=".UI.Crew.CrewChatActivity" />
        <activity android:name=".UI.Crew.CrewGamesActivity" />
        <activity android:name=".UI.Crew.CrewLeaderActivity"></activity>
    </application>

</manifest>